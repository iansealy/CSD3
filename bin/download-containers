#!/usr/bin/env bash

for module in `find $HOME/privatemodules -type f`; do
  container=`grep '\.sif$' $module | sed -e 's/^ *#*//' | awk '{ print $NF }'`
  if [ ! -f "$HOME/containers/$container" ]; then
    curl -Sso "$HOME/containers/$container" "https://csd3.buschlab.org/containers/$container"
  fi
done
